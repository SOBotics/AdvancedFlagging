{
  "extends": "tslint:latest",
  "rules": {
    "quotemark": [
      true,
      "single"
    ],
    "ordered-imports": false,
    "prefer-for-of": false,
    "object-literal-sort-keys": false,
    "trailing-comma": false,
    "interface-name": false,
    "align": false,
    "arrow-parens": false,
    "max-line-length": false,
    "array-type": false,
    "no-bitwise": false,
    "no-submodule-imports": false,
    "no-implicit-dependencies": false
  }
}